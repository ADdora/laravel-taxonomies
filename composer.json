{
    "name": "hamedov/laravel-taxonomies",
    "description": "Assign categories, tags, etc, to eloquent models.",
    "keywords": ["laravel", "taxonomies", "categories", "tags", "attributes"],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.21",
        "illuminate/database": "^10.21",
        "spatie/laravel-medialibrary": "^10.12",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-translatable": "^6.5",
        "staudenmeir/laravel-adjacency-list": "^1.11"
    },
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hamedov93/laravel-taxonomies/issues",
        "source": "https://github.com/hamedov93/laravel-taxonomies"
    },
    "authors": [
        {
            "name": "Mohamed Hamed",
            "email": "m.hamed@nezam.io"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Hamedov\\Taxonomies\\TaxonomyServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Hamedov\\Taxonomies\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
